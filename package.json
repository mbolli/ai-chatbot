{
    "name": "ai-chatbot-frontend",
    "version": "1.0.0",
    "description": "AI Chatbot frontend with TypeScript and esbuild",
    "scripts": {
        "build": "esbuild src/ts/main.ts --bundle --outfile=public/js/app.js --minify --sourcemap",
        "watch": "esbuild src/ts/main.ts --bundle --outfile=public/js/app.js --watch --sourcemap",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "esbuild": "^0.27.2",
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "@mbolli/datastar-attribute-on-keys": "^1.1.0"
    }
}
